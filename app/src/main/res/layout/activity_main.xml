<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bc2d"
    tools:context="com.example.andreia.testandoaviso.MainActivity">

    <Button
        android:id="@+id/btnMainIniciar"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/btnMainDescricao"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="21dp"
        android:background="@drawable/geht"/>

    <ImageButton
        android:id="@+id/btnMainDescricao"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="24dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/dessss"
        android:onClick="onCLickTelaDescricao" />

    <ImageButton
        android:id="@+id/btnMainHistorico"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/btnMainIniciar"
        android:layout_marginEnd="32dp"
        android:onClick="onCLickTelaHistorico"
        android:background="@drawable/asdasd" />


</RelativeLayout>
